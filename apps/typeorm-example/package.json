{
  "name": "example-typeorm-to-olap",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:moose": "moose-cli build",
    "build:all": "npm run build && moose-cli build",
    "update-moose": "pnpm update @514labs/moose-lib @514labs/moose-cli",
    "start-pg": "docker compose -f docker-compose.oltp.yaml up -d",
    "stop-pg": "docker compose -f docker-compose.oltp.yaml down",
    "setup-db": "tsx src/setup-db.ts",
    "setup-cdc": "./setup.sh",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist dist-moose"
  },
  "dependencies": {
    "@514labs/moose-lib": "^0.6.182",
    "@oltp-olap/shared": "workspace:*",
    "@scalar/express-api-reference": "^0.8.22",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "ts-patch": "^3.3.0",
    "typeorm": "^0.3.17",
    "typia": "^9.6.1"
  },
  "devDependencies": {
    "@514labs/moose-cli": "^0.6.182",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
