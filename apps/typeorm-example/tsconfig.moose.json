{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist-moose",
    "plugins": [
      {
        "transform": "./node_modules/@514labs/moose-lib/dist/compilerPlugin.js",
        "transformProgram": true
      },
      {
        "transform": "typia/lib/transform"
      }
    ]
  },
  "include": ["app/**/*"],
  "exclude": ["node_modules", "dist", "dist-moose"]
}
